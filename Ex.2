import java.util.Scanner;

public class PolygonCheck {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Введіть кількість кутів: ");
        int n = scanner.nextInt();
        int[] angles = new int[n];

        int sum = 0;
        System.out.println("Введіть кути багатокутника:");
        for (int i = 0; i < n; i++) {
            angles[i] = scanner.nextInt();
            sum += angles[i];
        }

        int validSum = 180 * (n - 2);
        if (sum == validSum) {
            System.out.println("Такий багатокутник можливий.");
        } else {
            System.out.println("Такий багатокутник неможливий.");
        }

        scanner.close();
    }
}
