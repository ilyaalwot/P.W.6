public class SineTable {
    public static void main(String[] args) {
        System.out.println("Таблиця синусів:");

        for (int i = 0; i <= 90; i++) {
            System.out.printf("%-8d %.5f  ", i, Math.sin(Math.toRadians(i)));
            if (i % 10 == 9) {
                System.out.println();
            }
        }
    }
}
